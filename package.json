{
  "name": "personal_website",
  "version": "1.0.0",
  "description": "My personal website",
  "author": "Tyler Peterson <tylermpeterson36@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=master --cleanup",
