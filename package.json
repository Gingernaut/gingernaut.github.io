    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.8.3",
    "push-dir": "^0.4.1",
    "sass-loader": "^6.0.7",
    "stylelint": "^9.2.0",
    "stylelint-scss": "^3.0.0"
  }
